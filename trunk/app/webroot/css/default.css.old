/* Boxed Style */

.boxed {
    margin-left: 5px;
    margin-bottom: 10px;
    height: 100%;
}

.boxed .title {
	height: 31px;
	padding: 10px 0 0 10px;
    text-align: center;
	background: #6FBE6E url(../img/img05.gif) repeat-x;
	font-size: 1.2em;
	color: #CC0000;
}

.boxed .content {
	padding: 10px 10px 30px 10px;
    text-align: left;
    height: 100%;
    border: 1px solid #FFEBCD;
	background: url(../img/img06.gif) repeat-x;
}

.boxed .menu {
	padding: 10px 10px 10px 10px;
	background: url(../img/img06.gif) repeat-x;
}

.boxed .menu ul {
	margin: 0;
	list-style: none;
}

.boxed .menu h3 {
	margin: 0;
	font: bold x-small Verdana, Arial, Helvetica, sans-serif;
}

.boxed .menu p {
/*	margin-bottom: 1.4em;*/
	line-height: normal;
}
/* Post */

.post {
    margin-left: 5px;
    text-align: left;
    height: 100%;
}

.post .title {
	padding: 5px 0 0 20px;
	font-size: 2em;
	font-weight: normal;
}


.post .story {
	padding: 20px 20px 10px 20px;
	background: url(../img/img07.gif) repeat-x;
	height:100%;
}

.post .story p, .post .story blockquote, .post .story ul, .post .story ol {
	margin-bottom: 1.5em;
	line-height: 180%;
}

/* Header */

#header {
	width: 740px;
	height: 150px;
	margin: 0 auto;
}

/* Menu */

#menu {
	width: 900px;
	height: 90px;
	margin: 0 auto;
}

#menu ul {
	margin: 0;
	padding-top: 25px;
	list-style: none;
}

#menu li {
	display: inline;
}

#menu a {
	display: block;
	float: left;
	height: 32px;
	margin-left: 1px;
	padding: 13px 10px 0 8px;
	background: #FFFFFF url(../img/img02.gif) repeat-x;
	text-align: center;
	text-decoration: none;
	font-weight: bold;
	color: #586BAA;
}

#menu a:hover {
	background-image: url(../img/img03.gif);
	border-color: #FEDA8D;
	color: #CC0000;
}

#menu .active a {
	height: 36px;
	margin-top: -4px;
	background-image: url(../img/img04.gif);
	border: 1px solid #479AC6;
	border-bottom: none;
	font-weight: bold;
	color: #479AC6;
}

